<h1>Listing car_prices</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @car_prices.each do |car_price| %>
  <tr>
    <td><%= car_price.name %></td>
    <td><%= link_to 'Show', car_price %></td>
    <td><%= link_to 'Edit', edit_car_price_path(car_price) %></td>
    <td><%= link_to 'Destroy', car_price, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

