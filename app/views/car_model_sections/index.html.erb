<h1>Listing car_model_sections</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Car make</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @car_model_sections.each do |car_model_section| %>
  <tr>
    <td><%= car_model_section.name %></td>
    <td><%= car_model_section.car_make_id %></td>
    <td><%= link_to 'Show', car_model_section %></td>
    <td><%= link_to 'Edit', edit_car_model_section_path(car_model_section) %></td>
    <td><%= link_to 'Destroy', car_model_section, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

